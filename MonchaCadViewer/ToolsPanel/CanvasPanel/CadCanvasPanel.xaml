<UserControl x:Class="MonchaCadViewer.ToolsPanel.CanvasPanel.CadCanvasPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MonchaCadViewer.ToolsPanel.CanvasPanel" xmlns:canvasobj="clr-namespace:MonchaCadViewer.CanvasObj"
             mc:Ignorable="d" 
             d:DesignHeight="400" d:DesignWidth="400">
    <Grid>
        <Viewbox ClipToBounds="True" Width="400" Height="400" Stretch="None" >
            <canvasobj:CadCanvas x:Name="Canvas"  Background="White"/>
        </Viewbox>
    </Grid>
</UserControl>
