<Window x:Class="CadProjectorViewer.Dialogs.IpCamDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CadProjectorViewer.Dialogs"
        mc:Ignorable="d"
        Title="IpCamDialog" Height="450" Width="800">
    <DockPanel>
        <DockPanel DockPanel.Dock="Top"
                   Margin="20">
            <Button DockPanel.Dock="Right"
                    Content="Show" Click="Button_Click"/>
            <TextBox x:Name="AdressBox"
                     DockPanel.Dock="Left"
                     FontSize="18"/>
        </DockPanel>
        <Image DockPanel.Dock="Bottom"/>
    </DockPanel>
</Window>
