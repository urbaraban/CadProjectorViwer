<Window x:Class="MonchaCadViewer.RequestLicenseCode"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MonchaCadViewer"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="RequestLicenseCode" Height="150" Width="400">
    <Grid>
        <TextBox x:Name="RequestBox" Height="31" Margin="10,10,12,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" VerticalContentAlignment="Center" MaxLines="1" IsEnabled="False"/>
        <TextBox x:Name="KeyBox" Height="31" Margin="10,46,12,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top"
                 VerticalContentAlignment="Center" MaxLines="1" TextChanged="KeyBox_TextChanged"/>
        <Button x:Name="OkBTn" Content="Ok" Margin="0,82,12,0" VerticalAlignment="Top" Height="27" HorizontalAlignment="Right" Width="90"/>
        <Label x:Name="SuccefulLbl" Content="Check" Margin="10,82,107,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
